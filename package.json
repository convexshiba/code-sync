{
	"name": "code-sync",
	"displayName": "code-sync",
	"description": "Sync VSCode extensions using OneDrive",
	"version": "0.0.1",
	"publisher": "golf1052",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.exportExtensions",
				"title": "Export Extensions"
			},
			{
				"command": "extension.listMissingInstalled",
				"title": "List Missing Installed Extensions"
			},
			{
				"command": "extension.listMissingExternal",
				"title": "List Missing External Extensions"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.7.3",
		"vscode": "0.10.x"
	}
}